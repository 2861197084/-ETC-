# Vanna Text2SQL 服务环境变量

# LLM 配置 (二选一)
LLM_PROVIDER=openai  # openai 或 ollama

# OpenAI 配置
OPENAI_API_KEY=sk-your-api-key-here
OPENAI_MODEL=gpt-4o-mini

# Ollama 配置 (本地部署)
OLLAMA_HOST=http://localhost:11434
OLLAMA_MODEL=qwen2.5:7b

# MySQL 配置 (通过 ShardingSphere)
MYSQL_HOST=shardingsphere
MYSQL_PORT=3307
MYSQL_USER=root
MYSQL_PASSWORD=root
MYSQL_DATABASE=etc

# ChromaDB 向量存储路径
CHROMA_PATH=./chroma_data

# 服务配置
VANNA_HOST=0.0.0.0
VANNA_PORT=8100
